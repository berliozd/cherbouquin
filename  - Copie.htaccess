

# BEGIN WordPress
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /cherbouquin/


# Rewrites for Zend modules
RewriteRule ^default(.)*$ public/index.php [L]
RewriteRule ^member(.)*$ public/index.php [L]

# Rewrites for Zend controllers
RewriteRule ^$ public/index.php [L]                     # module:default / controller:index / action:index
RewriteRule ^index/log$ public/index.php [L]            # module:default / controller:index / action:log
RewriteRule ^books(.)*$ public/index.php [L]            # module:default / controller:books / action:index
RewriteRule ^livres(.)*$ public/index.php [L]           # module:default / controller:books(route) / action:index
RewriteRule ^error(.)*$ public/index.php [L]            
RewriteRule ^wished-userbook(.)*$ public/index.php [L]  # module:default / controller:wished-userbooks
RewriteRule ^userbook-gift(.)*$ public/index.php [L]    # module:default / controller:userbook-gift
RewriteRule ^userbook-gifts(.)*$ public/index.php [L]   # module:default / controller:userbook-gifts
RewriteRule ^test(.)*$ public/index.php [L]
RewriteRule ^library(.)*$ public/index.php [L]          # module:default / controller:userbook-gift
RewriteRule ^book-search(.)*$ public/index.php [L]
RewriteRule ^userbook(.)*$ public/index.php [L]

RewriteRule ^membre/a-la-page(.)*$ public/index.php [L]
RewriteRule ^book(.)*$ public/index.php [L]

RewriteRule ^livre/([a-z0-9\-]+)/([a-z0-9\-]+)/(.*)$ /bibliotheque\-detail/\?page=book\%2Fview&bid=$3
RewriteRule ^index\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /cherbouquin/index.php [L]
</IfModule>

# END WordPress

FileETag MTime Size
<ifmodule mod_expires.c>
  <filesmatch "\.(jpg|gif|png|css|js|ico)$">
       ExpiresActive on
       ExpiresDefault "access plus 1 year"
   </filesmatch>
</ifmodule>
